{


    "name": "Calendar",
    "version": "0.1",
    "description": "Makes a calendar for MS Teams Educations accounts with no access to an assignment calendar",

    "permissions": ["storage"],
    "background": {
        "scripts": [
            "scripts/calendar.js",
            "scripts/dateConsts.js",
            "scripts/helper.js",
            "scripts/main.js"
        ],
        "persistent": false
    },

    "browser_action": {
        "default_popup": "popup.html"
    },
    
    "manifest_version": 2

}